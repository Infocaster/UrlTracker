{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Unattended": {
        "UpgradeUnattended": true,
        "InstallUnattended": true,
        "UnattendedUserName": "Test",
        "UnattendedUserEmail": "test@test.nl",
        "UnattendedUserPassword": "1234567890"
      },
      "Hosting": {
        "Debug": true
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "Id": "2fa8c982-7c9f-41d4-abb4-35c1ace9ef55"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Examine": {
        "LuceneDirectoryFactory": "TempFileSystemDirectoryFactory"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnFirstBoot": true
    }
  },
  "UrlTracker": {
    "HasDomainOnChildNode": false,
    "AppendPortNumber": false,
    "Enable": true,
    "Backoffice": {
      "Notifications": {
        "Enable": false
      }
    },
    "Pipeline": {
      "Enable": true,
      "EnableClientErrorTracking": true
    },
    "BlockedUrlsList": [
      "example.com",
      "example.nl"
    ]
  }
}